class Node:
    def __init__(self, data):
        self.data = data
        self.prev = None
        self.next = None

class NodeList:
    def __init__(self):
        self.head = None
        self.tail = None
        
    def unshift(self, data):
        node = Node(data)
        if self.head == None:
            self.head = self.tail = node
            self.head.prev = None
            self.head.next = None
        else:
            self.head = self.head.prev = node
            self.prev = None
    


            


nodes = NodeList()
nodes.unshift(1)
nodes.unshift(2)
nodes.unshift(3)
nodes.unshift(100)
print(nodes.head.data)
print(nodes.tail.data)
print(nodes.head.prev.data)
